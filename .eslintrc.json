{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [2, "tab"],
        "semi": [2, "always"],
        "comma-dangle": [2, "always-multiline"],
        "max-lines": [2, 80],
        "max-len": [2, 150],
        "no-trailing-spaces": 2
    }
}
